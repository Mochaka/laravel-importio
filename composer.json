{
    "name": "mochaka/laravel-importio",
    "description": "Simple interface to http://import.io for Laravel",
    "keywords": ["laravel", "importio","scraping", "guzzle"],
    "authors": [
        {
            "name": "Aaron Florey",
            "email": "azza@jcaks.net"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.1.*",
	"guzzlehttp/guzzle": "3.8.x"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Mochaka\\Importio\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
